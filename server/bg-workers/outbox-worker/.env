# Poll interval (ms)
OUTBOX_WORKER_POLL_MS=800

# Max events fetched per cycle
OUTBOX_WORKER_BATCH_SIZE=100

# Lease timeout for stale in_progress rows
OUTBOX_WORKER_LEASE_TIMEOUT_SECONDS=60

# Postgres
DATABASE_URL=postgres://cliprelay:cliprelay@cliprelay-postgres:5432/cliprelay?sslmode=disable

# MQ (RabbitMQ)
MQ_URL=amqp://cliprelay:cliprelay@cliprelay-rabbitmq:5672
MQ_QUEUE=cliprelay.events
